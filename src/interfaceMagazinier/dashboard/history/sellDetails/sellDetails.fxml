<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: #F8F8F8;" stylesheets="@../../../sells/imSells.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaceMagazinier.dashboard.history.sellDetails.sellDetailsController">
   <children>
      <TableView fx:id="table" prefHeight="300.0" prefWidth="1000.0">
         <columns>
            <TableColumn fx:id="barcodeColumn" prefWidth="130.0" text="Barcode" />
            <TableColumn fx:id="nameColumn" prefWidth="220.0" text="Name" />
            <TableColumn fx:id="quantityColumn" prefWidth="150.0" text="Quantity" />
            <TableColumn fx:id="sellpriceColumn" prefWidth="150.0" text="Price" />
            <TableColumn fx:id="profitColumn" prefWidth="150.0" text="Profit" />
            <TableColumn fx:id="expirationdateColumn" minWidth="0.0" prefWidth="200.0" text="Expiration date" />
         </columns>
      </TableView>
      <VBox layoutY="300.0" prefHeight="300.0" prefWidth="500.0" spacing="30.0">
         <children>
            <Label fx:id="sellIdLabel" text="Sell id : " />
            <Label fx:id="numberOfProductsLabel" layoutX="40.0" layoutY="40.0" text="Number of products : " />
            <Label fx:id="sellPriceLabel" layoutX="40.0" layoutY="87.0" text="Sell price : " />
            <Label fx:id="sellProfitLabel" layoutX="40.0" layoutY="134.0" text="Sell profit : " />
            <Label fx:id="sellTimeLabel" layoutX="40.0" layoutY="181.0" text="Sell Time : " />
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
      <VBox layoutX="500.0" layoutY="300.0" prefHeight="300.0" prefWidth="500.0" spacing="30.0">
         <children>
            <Label fx:id="discountLabel" text="Discount : " />
            <Label fx:id="loyalityCardLabel" layoutX="40.0" layoutY="40.0" text="Loyality card :" />
            <Label fx:id="discountAmountLabel" text="Discount amount : " />
            <Label fx:id="clientIdLabel" text="Client id : " />
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
