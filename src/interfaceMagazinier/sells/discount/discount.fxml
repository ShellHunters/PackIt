<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaceMagazinier.sells.discount.discountController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0" spacing="30.0">
         <children>
            <JFXTextField fx:id="discountPercent" focusColor="#ee974c" promptText="discount by %" />
            <JFXTextField fx:id="discountDA" focusColor="#ee974c" layoutX="30.0" layoutY="70.0" promptText="discount by DA" />
            <JFXButton onAction="#apply" style="-fx-background-color: #ee974c;" text="Apply" textFill="WHITE" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
